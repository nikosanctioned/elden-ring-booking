{
  "compilerOptions": {
    "noImplicitAny": false,
    "target": "ES2015",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      "@authtypes/*": ["./node_modules/@auth/core/src/*"]
    }
  },
  "exclude": [
    "node_modules",
    "node_modules/@auth/core/src/lib/pages/signin.tsx"
  ],
  "include": ["next-env.d.ts", ".next/types/**/*.ts", "**/*.tsx"]
}
